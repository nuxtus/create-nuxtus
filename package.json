{
  "name": "create-nuxtus",
  "description": "npx command for generating a new Nuxtus boilerplate website.",
  "version": "1.3.1",
  "exports": "./build/src/main.js",
  "type": "module",
  "engines": {
    "node": "^20.0.0",
    "npm": ">=8.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxtus/create-nuxtus.git"
  },
  "keywords": [
    "Directus",
    "Nuxt",
    "boilerplate",
    "Tailwind",
    "Tailwindcss"
  ],
  "author": "Craig Harman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuxtus/create-nuxtus/issues"
  },
  "homepage": "https://github.com/nuxtus/create-nuxtus#readme",
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "bin": {
    "create-nuxtus": "./build/src/main.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "child_process": "^1.0.2",
    "command-exists": "^1.2.9",
    "figlet": "^1.5.2",
    "inquirer": "^9.1.0",
    "joi": "^17.6.0",
    "liquidjs": "^10.0.0",
    "nanoid": "^5.0.0",
    "nunjucks": "^3.2.3",
    "ora": "^8.0.0",
    "rimraf": "^5.0.0",
    "tslib": "~2.6.0",
    "uuid": "^9.0.0"
  },
  "release": {
    "branches": "main",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "revert",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "minor"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "Changelog"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.6",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "semantic-release": "23.0.2",
    "ts-jest": "29.1.2",
    "tsutils": "3.21.0",
    "typescript": "5.3.3"
  },
  "volta": {
    "node": "20.15.0"
  }
}
